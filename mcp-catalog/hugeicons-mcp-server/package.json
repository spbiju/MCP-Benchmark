    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.9.0",
    "axios": "^1.8.4",
    "fuse.js": "^7.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/node": "^20.17.30",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "hugeicons",
    "mcp",
    "icons",
    "model-context-protocol"
  ],
  "author": "Mohamed Tawfik (https://hugeicons.com)",
  "homepage": "https://hugeicons.com",
  "license": "MIT"
}

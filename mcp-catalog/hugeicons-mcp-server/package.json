{
  "name": "@hugeicons/mcp-server",
  "version": "0.1.2",
  "description": "MCP server for Hugeicons search and usage documentation",
  "type": "module",
  "bin": {
    "hugeicons-mcp": "build/index.js"
  },
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && chmod +x build/index.js",
